function init_map() {
    time set 0;
    kill @e[type=item];
    gamerule doDaylightCycle true;
    Destroyables.placeAll();
}

function reset_map() {
    Destroyables.removeAll();
    gamerule doDaylightCycle false;
    kill @e[type=item];
    time set 0;
}
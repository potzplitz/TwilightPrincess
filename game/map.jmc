function init_map() {
    time set 0;
    kill @e[type=item];
    gamerule doDaylightCycle true;
    Grass.place();
}

function reset_map() {
    Grass.remove();
    gamerule doDaylightCycle false;
    kill @e[type=item];
    time set 0;
}